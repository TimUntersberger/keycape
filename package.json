{
  "name": "keycape",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts",
    "fresh:start": "rm config.prev.yaml; docker-compose down && docker-compose up -d && npm start",
    "build": "tsc",
    "orm": "mikro-orm",
    "migration:create": "mikro-orm migration:create",
    "fresh:migration:create": "rm -rf migrations; rm -rf temp; mikro-orm migration:create"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.3.7",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^9.0.0",
    "ky": "^0.17.0",
    "ky-universal": "^0.4.0",
    "mikro-orm": "^3.1.0",
    "pg": "^7.18.1",
    "reflect-metadata": "^0.1.13",
    "simple-oauth2": "^3.3.0",
    "typedi": "^0.8.0",
    "yaml": "^1.7.2"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/orm-config.ts"
    ]
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.2",
    "@types/joi": "^14.3.4",
    "@types/node": "^13.7.0",
    "@types/simple-oauth2": "^2.5.2",
    "@types/yaml": "^1.2.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
